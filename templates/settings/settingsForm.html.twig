<div class="form-wrapper">
    {{ form_start(settingsForm) }}
    <div class="row">
        <div class="col">
            {{ form_row(settingsForm.apiKey) }}
        </div>
    </div>
    <div class="row">
        <div class="col">
            {{ form_row(settingsForm.weatherInfoByCityAndStateLink) }}
        </div>
    </div>
    <div class="row">
        <div class="col">
            {{ form_row(settingsForm.weatherInfoByCityIdLink) }}
        </div>
    </div>
    <div class="row">
        <div class="col">
            {{ form_row(settingsForm.weatherInfoByCityLink) }}
        </div>
    </div>
    <div class="row">
        <div class="col">
            {{ form_row(settingsForm.save, {'attr': {'class': 'btn-block btn btn-primary searchFormBtn'}}) }}
        </div>
    </div>
    {{ form_end(settingsForm) }}
</div>